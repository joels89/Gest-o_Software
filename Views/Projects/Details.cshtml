@model Gestao_Software.Models.Project

@{
    ViewData["Title"] = "Details";
}

<h1>Detalhes</h1>

<h5>Projecto</h5>
<br />

<dl class="row">
    <dt class="col-2">
        Nome
    </dt>
    <dd class="col-xl-2">
        @Html.DisplayFor(model => model.Name)
    </dd>
    <dt class="col-2">
        Data Inicial
    </dt>
    <dd class="col-xl-2">
        @Html.DisplayFor(model => model.BeginDate)
    </dd>
    <dt class="col-2">
        Data Final
    </dt>
    <dd class="col-xl-2">
        @Html.DisplayFor(model => model.EndDate)
    </dd>
    <dt class="col-2">
        Cliente
    </dt>
    <dd class="col-xl-2">
        @Html.DisplayFor(model => model.Client.Nome)
    </dd>
</dl>

<br />

<div>
    <a asp-action="Index" class="btn btn-secondary btn-sm">&laquo; Voltar &raquo;</a>
</div>
